{
  "name": "Cornucopia Enhancement Suite",
  "version": "0.0.2",
  "description": "Various enhancements to Cornucopia.se",
  "manifest_version": 3,
  "author": "Andreas Lundgren",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["https://cornucopia.se/*"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
